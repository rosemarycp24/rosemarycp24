import yaml

# Load the YAML data from your file
with open('your_file.yaml', 'r') as yaml_file:
    yaml_data = yaml.safe_load(yaml_file)

# Extract URLs from the Build section
build_urls = []
abinitio_data = yaml_data.get('Build', {}).get('abinitio', {})
if abinitio_data:
    eme_projects_list = abinitio_data.get('EMEProjectsList', [])
    for item in eme_projects_list:
        if isinstance(item, str) and item.startswith('http'):
            build_urls.append(item)

# Extract URLs from the Rollback section
rollback_urls = []
rollback_data = yaml_data.get('Rollback', {}).get('database', {})
if rollback_data:
    database_scripts = rollback_data.get('DatabaseScripts', [])
    for item in database_scripts:
        if isinstance(item, str) and item.startswith('http'):
            rollback_urls.append(item)

# Print the extracted URLs
print("Build URLs:")
for url in build_urls:
    print(url)

print("\nRollback URLs:")
for url in rollback_urls:
    print(url)
